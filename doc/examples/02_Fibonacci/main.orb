import "base.orb";
import "clib.orb";

fnc fibonacci::evaluable (n:u32) u32 {
    if (< n 2) {
        ret n;
    };

    sym a:(u32 3);
    = ([] a 0) 0;
    = ([] a 1) 1;

    repeat (- n 1) {
        = ([] a 2) (+ ([] a 0) ([] a 1));
        
        = ([] a 0) ([] a 1);
        = ([] a 1) ([] a 2);
    };

    ret ([] a 2);
};

fnc main () () {
    printf "%u\n" (fibonacci 20);
};