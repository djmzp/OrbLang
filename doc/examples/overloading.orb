import "clib.orb";

fnc print (x:i32) () {
    printf "%d\n" x;
};

fnc print (x:f64) () {
    printf "%.4f\n" x;
};

mac inc (x::preprocess) {
    ret \(= ,x (+ ,x 1:(typeOf x)));
};

mac inc (x::preprocess k::preprocess) {
    ret \(= ,x (+ ,x ,k));
};

fnc main () () {
    sym (a 100);
    inc a;
    inc a 10;
    print a; # 111

    sym (b 123.4567);
    print b; # 123.4567
};