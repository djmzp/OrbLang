import "tests/clib.orb";

fnc println() { (putchar '\n'); }

fnc print_i8(i8 x) { (printf "%d" x); }
fnc println_i8(i8 x) { (printf "%d\n" x); }
fnc scan_i8() i8 { let x:i8; (scanf "%hhd" (& x)); ret x; }

fnc print_i16(i16 x) { (printf "%d" x); }
fnc println_i16(i16 x) { (printf "%d\n" x); }
fnc scan_i16() i16 { let x:i16; (scanf "%hd" (& x)); ret x; }

fnc print_i32(i32 x) { (printf "%d" x); }
fnc println_i32(i32 x) { (printf "%d\n" x); }
fnc scan_i32() i32 { let x:i32; (scanf "%d" (& x)); ret x; }

fnc print_i64(i64 x) { (printf "%d" x); }
fnc println_i64(i64 x) { (printf "%d\n" x); }
fnc scan_i64() i64 { let x:i64; (scanf "%d" (& x)); ret x; }

fnc print_u8(u8 x) { (printf "%u" x); }
fnc println_u8(u8 x) { (printf "%u\n" x); }
fnc scan_u8() u8 { let x:u8; (scanf "%hhu" (& x)); ret x; }

fnc print_u16(u16 x) { (printf "%u" x); }
fnc println_u16(u16 x) { (printf "%u\n" x); }
fnc scan_u16() u16 { let x:u16; (scanf "%hu" (& x)); ret x; }

fnc print_u32(u32 x) { (printf "%u" x); }
fnc println_u32(u32 x) { (printf "%u\n" x); }
fnc scan_u32() u32 { let x:u32; (scanf "%u" (& x)); ret x; }

fnc print_u64(u64 x) { (printf "%u" x); }
fnc println_u64(u64 x) { (printf "%u\n" x); }
fnc scan_u64() u64 { let x:u64; (scanf "%u" (& x)); ret x; }

fnc print_f32(f32 x) { (printf "%.4f" (cast f64 x)); }
fnc println_f32(f32 x) { (printf "%.4f\n" (cast f64 x)); }
fnc scan_f32() f32 { let x:f32; (scanf "%f" (& x)); ret x; }

fnc print_f64(f64 x) { (printf "%.4lf" x); }
fnc println_f64(f64 x) { (printf "%.4lf\n" x); }
fnc scan_f64() f64 { let x:f64; (scanf "%lf" (& x)); ret x; }

fnc print_c8(c8 x) { (putchar x); }
fnc println_c8(c8 x) { (putchar x); (println); }
fnc scan_c8() c8 { ret (getchar); }

fnc print_str(c8 cn[] str) { (printf str); }
fnc println_str(c8 cn[] str) { (printf str); (println); }
