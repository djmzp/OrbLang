import "util/io.orb";

eval (fnc foo () raw {
    ret ();
});

eval (fnc bar (r:raw) raw {
    ret r;
});

fnc main () () {
    ;
    eval (sym (a ()) b:raw (c:(raw cn) a));
    = b a;
    eval (sym (d:(raw 2)));
    = ([] d 0) a;
    = ([] d 1) (foo);
    = b (bar a);
    = a \(+ 1 2);
    = b \();

    \();
    \\();
    ,();
    ,,();
    \,();
    ,\();

    sym (x:i32 100);
    println_i32 \,x;
    println_i32 \,(+ x 1);
    println_i32 ,\,,\\(+ x 2);
    println_i32 ,(+ \x 3);
    println_i32 ,(+ ,x 4);
    println_i32 ,(+ \,x 5);
    println_i32 ,(+ ,\x 6);
    block {
        \,(println_i32 (+ x 7))
    };
    block {
        ,\(println_i32 (+ x 8))
    };

    = a \(200 -1);
    println_i32 (. a 0);
    println_i32 (. \(-1 201) 1);
    = x 202;
    println_i32 (. \(abc ,x) 1);
    = x 203;
    = a \(abc ,x);
    println_i32 (. a 1);
    = x 204;
    = a \(\(abc ,,x));
    println_i32 (. a 0 1);
    = (. a 0) \(205);
    println_i32 (. a 0 0);

    println_i32 (. (+ \(abc def) \(300 -1)) 2);
    = a \(abc def);
    println_i32 (. (+ a \(-1 301)) 3);
    = a (+ \(302) a);
    println_i32 (. a 0);
};