    = (. f0 x) x;
});

eval (fnc foo7 (x:i32) Foo {
    sym f:Foo;
    = (. f x) x;
    ret (>> f);
});

eval (fnc foo8 (x:i32) Foo {
    ret (block Foo {
        sym f:Foo;
        = (. f x) x;
        pass (>> f);
    });
});

mac foo9 (x) {
