import "util/io.orb";

eval (sym glob:i32);

fnc main () () {
    eval (sym a:i32);
    = a 100;
    println_i32 a;
    = (* (& a)) 101;
    println_i32 a;

    eval (sym b:(i32 2));
    = ([] b 0) 200;
    println_i32 ([] b 0);
    = ([] b 0) 201;
    println_i32 ([] b 0);
    = ([] b 1) 202;
    println_i32 ([] b 1);

    eval (sym (d (& b)));
    = ([] (* d) 1) 300;
    println_i32 ([] b 1);
    = ([] (* d) 1) 301;
    println_i32 ([] (* d) 1);

    eval (sym (e (& a)));
    = (* e) 400;
    println_i32 a;
    = (* e) 401;
    println_i32 (* e);

    eval (sym f:(i32 i32));
    = (. f 0) 500;
    println_i32 (. f 0);
    = (. f 0) 501;
    println_i32 (. f 0);
    = (. f 1) 502;
    println_i32 (. f 1);

    eval (sym (g (tup 0 (& f))));
    = (. (* (. g 1)) 0) 600;
    println_i32 (. (* (. g 1)) 0);
    = (. f 0) 601;
    println_i32 (. (* (. g 1)) 0);

    = glob 700;
    println_i32 glob;
};