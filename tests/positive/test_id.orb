import "util/io.orb";

fnc main () () {
    sym p:i32;

    sym (x 100);
    println_i32 (\x);

    = x 101;
    eval (sym (id0 \x));
    println_i32 (id0);

    = x 102;
    eval (sym (id1 \id0));
    println_i32 ((id1));

    eval (= id0 \a);
    = p 200;
    eval (block {
        exit (== id0 \a);
        = p -1;
    });
    println_i32 p;

    = p 201;
    eval (block {
        exit (!= id0 \b);
        = p -1;
    });
    println_i32 p;

    sym (a=+-*/%<>&|^!~[]._?0123456789:i32 300);
    println_i32 a=+-*/%<>&|^!~[]._?0123456789;
};