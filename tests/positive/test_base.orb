        println_i32 (+ 1600 (cast i32 y));
        = x y;
        println_i32 (+ 1600 (cast i32 (+ x y)));
        if (== (+ x y) Baz.two) {
            println_i32 1603;
        };

        println_i32 (+ 1601 (base.enumSize Baz));

        println_c8 (cast c8 Zab.a);
        println_c8 (cast c8 Zab.b);
    };
};