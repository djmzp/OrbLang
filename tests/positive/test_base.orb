    block {
        ?_ true Bar (make Bar (x 1200)) (make Bar (x -1));
        ?_ false Bar (make Bar (x -1)) (make Bar (x 1201));
    };

    block {
        arr Bar (make Bar (x 1300));
    };

    block {
        tup (make Bar (x 1401)) (make Bar (x 1400));
