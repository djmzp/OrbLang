    if false {
        println_i32 -1;
    } false {
        println_i32 -1;
    } {
        println_i32 104;
    };
    if false {
        println_i32 -1;
    } true {
        println_i32 105;
    } false {
        println_i32 -1;
    };
    if false {
        println_i32 -1;
    } true {
